import javax.crypto.*; 
import javax.crypto.spec.SecretKeySpec; 
import java.util.*; 
public class AES 
{ 
 public static void main(String[] args)throws Exception 
 { 
 String key="1234567890123456",msg="HELLO"; 
 Cipher c=Cipher.getInstance("AES"); 
 c.init(Cipher.ENCRYPT_MODE,new SecretKeySpec(key.getBytes(),"AES")); 
 byte[] enc=c.doFinal(msg.getBytes()); 
 System.out.println("Encrypted: "+Base64.getEncoder().encodeToString(enc)); 
 c.init(Cipher.DECRYPT_MODE,new SecretKeySpec(key.getBytes(),"AES")); 
 System.out.println("Decrypted: "+new String(c.doFinal(enc))); 
 } 
}
